{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": {
          "limit": 100,
          "matchAny": false,
          "tags": [],
          "type": "dashboard"
        },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum(ollama_api_requests_total{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"}) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "A"
        }
      ],
      "title": "Total Requests",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "avg(ollama_api_request_duration_seconds_sum{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"} / ollama_api_request_duration_seconds_count{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"}) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "B"
        }
      ],
      "title": "Average Request Latency (seconds)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 6 },
      "id": 3,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ollama_api_request_duration_seconds_bucket{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"}[5m])) by (le, endpoint, model, options, raw, streaming, format, thinking, http_status))",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "C"
        }
      ],
      "title": "Request Latency 95th Percentile",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 6 },
      "id": 4,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum(ollama_api_response_size_bytes_sum{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"}) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}}",
          "refId": "D"
        }
      ],
      "title": "Response Size (bytes)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 12 },
      "id": 5,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum by (endpoint, model, options, raw, streaming, format, thinking, http_status) (rate(ollama_api_requests_total{endpoint=~\"$endpoint\",http_status=~\"$status\",model=~\"$model\"}[1m]))",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "E"
        }
      ],
      "title": "Request Rate (requests per second)",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 41,
  "tags": ["HTTP", "API", "Performance", "Latency", "Throughput"],
  "templating": {
    "list": [
      {
        "name": "endpoint",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(ollama_api_requests_total, endpoint)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "sort": 1,
        "hide": 0,
        "label": "Endpoint"
      },
      {
        "name": "status",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(ollama_api_requests_total, http_status)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "sort": 1,
        "hide": 0,
        "label": "HTTP Status"
      },
      {
        "name": "model",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(ollama_api_requests_total, model)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "sort": 1,
        "hide": 0,
        "label": "Model"
      }
    ]
  },
  "time": { "from": "now-5m", "to": "now" },
  "title": "Ollama API Stats",
  "uid": "ollama-api-stats",
  "version": 2
}
