  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 },
      "id": 1,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum(ollama_api_requests_total) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "A"
        }
      ],
      "title": "Total Requests",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 },
      "id": 2,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "avg(ollama_api_request_duration_seconds_sum / ollama_api_request_duration_seconds_count) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "B"
        }
      ],
      "title": "Average Request Latency (seconds)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 6 },
      "id": 3,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ollama_api_request_duration_seconds_bucket[5m])) by (le, endpoint, model, options, raw, streaming, format, thinking, http_status))",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "C"
        }
      ],
      "title": "Request Latency 95th Percentile",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 6 },
      "id": 4,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum(ollama_api_response_size_bytes_sum) by (endpoint, model, options, raw, streaming, format, thinking, http_status)",
          "legendFormat": "{{endpoint}} - {{model}}",
          "refId": "D"
        }
      ],
      "title": "Response Size (bytes)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 12 },
      "id": 5,
      "options": { "legend": { "showLegend": true } },
      "targets": [
        {
          "expr": "sum by (endpoint, model, options, raw, streaming, format, thinking, http_status) (rate(ollama_api_requests_total[1m]))",
          "legendFormat": "{{endpoint}} - {{model}} - {{http_status}}",
          "refId": "E"
        }
      ],
      "title": "Request Rate (requests per second)",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 41,
  "tags": ["HTTP", "API", "Performance", "Latency", "Throughput"],
  "templating": { "list": [] },
  "time": { "from": "now-5m", "to": "now" },
